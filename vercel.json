{
    "version": 2,
    "name": "fullstack-shop-app",
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      },
      {
        "src": "/client/dist/*",
        "use":"@vercel/static"
      }
    ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
    ]
  }