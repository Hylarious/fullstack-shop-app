{
    "version": 2,
    "name": "fullstack-shop-app",
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      },
      {
        "src": "client/build/index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts"
      },
      {
        "src": "/static/(.*)",
        "headers": { "cache-control": "public, max-age=86400" },
        "dest": "client/build/static/$1"
      }
    ]
  }